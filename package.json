{
  "name": "workspace-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "set \"REACT_APP_USER_ENV=development\" && react-scripts start",
    "debug": "set \"REACT_APP_USER_ENV=debug\" && react-scripts start",
    "build": "set \"REACT_APP_USER_ENV=static\" && react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
